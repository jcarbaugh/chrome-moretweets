{
    "name": "More Tweets!",
    "version": "1.0.1",
    "description": "Automatically pull more tweets when scrolled to the bottom of a user's Twitter page",
    "content_scripts": [{
        "matches": [
            "http://*.twitter.com/*",
            "https://*.twitter.com/*"
        ],
        "js": ["contentscript.js"]
    }],
    "icons": {
        "19": "icons/moretweets-19x19.png",
        "32": "icons/moretweets-32x32.png",
        "64": "icons/moretweets-64x64.png",
        "128": "icons/moretweets-128x128.png"
    }
}